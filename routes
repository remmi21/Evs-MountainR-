# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.Mountain_Resorts()
GET     /User_Mountain_Resorts      controllers.Application.User_Mountain_Resorts()
GET     /Empl_Mountain_Resorts      controllers.Application.Empl_Mountain_Resorts()
GET     /Book_Mountain_Resorts      controllers.Application.Book_Mountain_Resorts()
GET     /add                        controllers.Application.Add()
GET     /addHotel                   controllers.Application.addHotel()
GET     /edit                       controllers.Application.Edit()
GET     /guestLogin                 controllers.Application.guestLogin()
GET     /emplLogin                  controllers.Application.workLogin()
GET     /delete                     controllers.Application.Delete()
GET     /Delete                     controllers.Application.DeleteHotel()
GET     /home                       controllers.Application.Home()
GET     /successBook                controllers.Application.SuccessBooking()
GET     /change                     controllers.Application.ChangeHotel()
GET     /book                       controllers.Application.Booking()
GET     /addroom                    controllers.Application.Addroom()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
